{
    "name": "likewares/laravel-mobile-detect",
    "description": "Accès instantané à la détection mobile directement à partir des modèles Blade.",
    "keywords": ["laravel", "mobile", "detection", "blade"],
    "license": "MIT",
    "authors": [
        {
            "name": "KOUAOBILE ASSANDE HUGUES",
            "email": "assandehugues@gmail.com",
            "homepage": "https://likewares.com",
            "role": "CEO Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Likewares\\MobileDetect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Likewares\\MobileDetect\\MobileDetectServiceProvider"
            ],
            "aliases": {
                "MobileDetect": "Likewares\\MobileDetect\\Facades\\MobileDetect"
            }
        }
    },
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "mobiledetect/mobiledetectlib": "^2.7|^2.8|^4.8.09"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.3",
        "phpspec/prophecy-phpunit": ">=1.1",
        "philo/laravel-blade": ">=3.1",
        "laravel/framework": ">=5.0"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
